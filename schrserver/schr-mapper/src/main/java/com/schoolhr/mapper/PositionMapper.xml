<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.schoolhr.mapper.PositionMapper">
    <insert id="insert"></insert>
    <insert id="insertSelective"></insert>
    <update id="updateByPrimaryKeySelective"></update>
    <update id="updateByPrimaryKey"></update>
    <delete id="deleteByPrimaryKey">
        delete from position
        where id = #{id,jdbcType=INTEGER}
    </delete>
    <delete id="deletePositionByIds">
        delete from position  where id in
        <foreach collection="ids" item="id" separator="," open="(" close=")">
            #{id}
        </foreach>
    </delete>
    <select id="selectByPrimaryKey" resultType="com.schoolhr.model.Position"></select>
    <select id="getAllPositions" resultType="com.schoolhr.model.Position">
        select * from position ;
    </select>
</mapper>