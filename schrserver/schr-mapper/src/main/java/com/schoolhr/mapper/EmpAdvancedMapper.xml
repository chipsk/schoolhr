<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.schoolhr.mapper.EmpAdvancedMapper">
    <select id="getAllEmpAdvanced" resultType="com.schoolhr.model.EmpAdvanced">
        select * from advancedemp
    </select>
    <select id="getEmpAdvancedByid" resultType="com.schoolhr.model.EmpAdvanced">
        select * from advancedemp where id=#{id}
    </select>
</mapper>