<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.schoolhr.mapper.ActManaMapper">

    <select id="getAllActions" resultType="com.schoolhr.model.ActManagement">
        select * from action
    </select>
    <select id="GetActionsByid" resultType="com.schoolhr.model.ActManagement">
        select * from action where id=#{id,jdbcType=INTEGER}
    </select>
</mapper>