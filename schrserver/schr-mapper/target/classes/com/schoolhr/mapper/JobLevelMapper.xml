<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.schoolhr.mapper.JobLevelMapper">
    <insert id="insert"></insert>
    <insert id="insertSelective"></insert>
    <update id="updateByPrimaryKeySelective"></update>
    <update id="updateByPrimaryKey"></update>
    <delete id="deleteByPrimaryKey"></delete>
    <delete id="deleteJobLevelsByIds">
        delete from joblevel  where id in
        <foreach collection="ids" item="id" separator="," open="(" close=")">
            #{id}
        </foreach>
    </delete>
    <select id="selectByPrimaryKey" resultType="com.schoolhr.model.JobLevel"></select>
    <select id="getAllJobLevels" resultType="com.schoolhr.model.JobLevel">
        select * from joblevel;
    </select>
</mapper>